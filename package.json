    "chalk": "5.2.0",
    "cli-progress": "3.10.0",
    "commander": "9.4.1",
    "filesize": "8.0.0",
    "handlebars": "4.7.7",
    "lodash": "4.17.21",
    "minipass": "^7.1.2",
    "moment": "^2.29.4",
    "moment-timezone": "0.5.38",
